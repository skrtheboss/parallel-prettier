{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/parallel-prettier/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/packages/parallel-prettier",
                "main": "packages/parallel-prettier/src/index.ts",
                "tsConfig": "packages/parallel-prettier/tsconfig.lib.json",
                "assets": ["packages/parallel-prettier/*.md"],
                "updateBuildableProjectDepsInPackageJson": false
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/parallel-prettier/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/packages/parallel-prettier"],
            "options": {
                "jestConfig": "packages/parallel-prettier/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "version": {
            "executor": "@jscutlery/semver:version",
            "options": {
                "baseBranch": "main",
                "preset": "conventional",
                "tagPrefix": "${projectName}@",
                "commitMessageFormat": "chore(${projectName}): release version ${version}"
            }
        }
    },
    "tags": []
}
